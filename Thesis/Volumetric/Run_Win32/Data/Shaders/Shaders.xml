      <Depth compare="Always" write="false" />

      <Defaults>
        <Texture fileName="Data/Images/Texture_Default.png" />
      </Defaults>
    </ShaderPass >
  </Shader>
  <Shader name="GodRays" >
    <ShaderPass queue="0" layer="0" >
      <Program fileName="Data/Shaders/GodRays" />

      <Blend>
        <Color operation="Add" source="srcAlpha" destination="oneMinusSrcAlpha" />
        <Alpha operation="Add" source="one" destination="one" />
      </Blend>

      <Depth compare="Always" write="false" />

      <Defaults>
        <Float  name="NUM_SAMPLES" defaultValue="200.0" />
        <Float  name="WEIGHT" defaultValue="0.05" />
        <Float  name="DECAY" defaultValue="0.95" />
        <Float  name="EXPOSURE" defaultValue="2.0" />
        <Texture fileName="Data/Images/Texture_Default.png" />
      </Defaults>
    </ShaderPass >
  </Shader>
  <!-- Compute shaders -->
  <Shader name="3DNoiseCompute">
    <ShaderPass compute="true" >
      <Program fileName="Data/Shaders/Compute/3DNoise" />
    </ShaderPass >
  </Shader>
